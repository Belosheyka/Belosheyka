using System;
using System.Numerics;

namespace ConsoleApp4
{
   class Program
   {
      private static void Main()
      {
         BigInteger a = 0;
         BigInteger b = 1;
         var count = 0;
         while (a.ToString().Length<1000)
         {
            var c = a + b;
            a = b;
            b = c;
            count++;
         }

         Console.WriteLine(count);
      }
   }
}
